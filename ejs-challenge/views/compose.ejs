<%- include("partials/header") -%>

<h1>Compose</h1>

<div class="container-fluid">
  <form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="postTitle" placeholder="title">
      <label>Post</label>
      <textarea class="form-control" name="postBody" rows="10" cols="50" placeholder="Write here.."></textarea>
    </div>
    <div class="form-group">
      <button class="btn btn-primary" type="submit" name="button">Pubish</button>
    </div>
  </form>
</div>


<%- include("partials/footer") -%>
